{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
     "lib": [
       "es2015",
       "dom"
     ],
     "declaration": true,
     "sourceMap": false,
     "outDir": "build/dist/es5",
     "removeComments": false,
     "strict": true,
     "noImplicitAny": true,
     "strictFunctionTypes": true,
     "alwaysStrict": true,
     "noUnusedLocals": true,
     "noImplicitReturns": true
  },
  "exclude": [
    "src/**/*.test.ts",
    "export-package.ts"
  ]
}